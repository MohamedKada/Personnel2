<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>

<AnchorPane prefHeight="270.0" prefWidth="300.0" styleClass="root" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.personnel.controllers.SuppLigueViewController">
   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="35.0" prefHeight="200.0" prefWidth="200.0" styleClass="content-pane">
         <children>
            <Label styleClass="title-label" text="Supprimer">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <Label text="Voulez vous vraiment supprimer la ligue ?">
               <VBox.margin>
                  <Insets bottom="30.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#loadOui" styleClass="button-danger" text="Oui" />
                  <Button mnemonicParsing="false" styleClass="button-secondary" text="Non" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
